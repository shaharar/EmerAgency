<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="460.0" prefWidth="620.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.WebsiteView">
   <children>
      <ImageView fitHeight="77.0" fitWidth="87.0" layoutX="7.0" layoutY="3.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@logo.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="574.0" fitWidth="460.0" layoutX="185.0" layoutY="1.0" opacity="0.58" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@155994848127060913.png" />
         </image>
      </ImageView>
      <Button fx:id="btn_Create" layoutX="240.0" layoutY="126.0" mnemonicParsing="false" onAction="#createEvent" prefHeight="39.0" prefWidth="198.0" text="Create New Event">
         <font>
            <Font name="Georgia Bold" size="18.0" />
         </font></Button>
      <Button fx:id="btn_Watch" layoutX="257.0" layoutY="274.0" mnemonicParsing="false" onAction="#watch" prefHeight="17.0" prefWidth="142.0" text="Watch Event">
         <font>
            <Font name="Georgia Bold" size="18.0" />
         </font></Button>
      <Button fx:id="btn_CreateUpdate" layoutX="257.0" layoutY="321.0" mnemonicParsing="false" onAction="#createUpdate" prefHeight="33.0" prefWidth="142.0" text="Add Update">
         <font>
            <Font name="Georgia Bold" size="18.0" />
         </font></Button>
      <Label fx:id="lbl_user" layoutX="573.0" layoutY="14.0" prefHeight="17.0" prefWidth="157.0" text="Username: ">
         <font>
            <Font name="Georgia Bold" size="12.0" />
         </font></Label>
      <Label fx:id="lbl_org" layoutX="573.0" layoutY="31.0" prefHeight="17.0" prefWidth="175.0" text="Organozation:">
         <font>
            <Font name="Georgia Bold" size="12.0" />
         </font></Label>
      <Label fx:id="lbl_rank" layoutX="573.0" layoutY="48.0" prefHeight="17.0" prefWidth="98.0" text="Rank: ">
         <font>
            <Font name="Georgia Bold" size="12.0" />
         </font></Label>
      <Label layoutX="153.0" layoutY="219.0" prefHeight="23.0" prefWidth="198.0" text="Choose an existing event">
         <font>
            <Font name="Georgia Bold" size="15.0" />
         </font></Label>
      <Button fx:id="btn_LogOut" layoutX="626.0" layoutY="65.0" mnemonicParsing="false" onAction="#logOut" text="Log Out" textFill="#e35d12">
         <font>
            <Font name="Georgia Bold" size="12.0" />
         </font></Button>
      <Label layoutX="153.0" layoutY="194.0" prefHeight="23.0" prefWidth="133.0" text="Choose category ">
         <font>
            <Font name="Georgia Bold" size="15.0" />
         </font>
      </Label>
      <ComboBox fx:id="categoryMenu" layoutX="356.0" layoutY="193.0" prefHeight="25.0" prefWidth="118.0" promptText="Categories">
      </ComboBox>
      <ComboBox fx:id="eventsMenu" layoutX="356.0" layoutY="224.0" prefHeight="25.0" prefWidth="118.0" promptText="Events" />
      <CheckBox fx:id="finishedChoosing" layoutX="486.0" layoutY="197.0" mnemonicParsing="false" onAction="#finishChoosingCategory" text="finished choosing" />
      <CheckBox fx:id="finishedChoosingEvent" layoutX="486.0" layoutY="229.0" mnemonicParsing="false" onAction="#finishChoosingEvent" text="finished choosing" />
   </children>
</AnchorPane>
